{
  "name": "fluid-move-language-support",
  "displayName": "FMW Language Support",
  "description": "Provides syntax highlighting for custom files with .fmw extensions.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "fmw",
        "aliases": [
          "Fluid Move",
          "fmw"
        ],
        "extensions": [
          ".fmw"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "fmw",
        "scopeName": "source.fmw",
        "path": "./syntaxes/fmw.tmLanguage.json"
      }
    ]
  }
}
